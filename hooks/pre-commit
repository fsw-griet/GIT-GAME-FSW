#!/bin/bash

file="final_submit.txt"

correct_key="d/e/1FA-ITZjUv54-!ds#we"

if [[ ! -f "$file" ]]; then
  echo "Error: $file does not exist."
  exit 1
fi

keys=$(cat "$file")

if [[ "$keys" == "$correct_key" ]]; then
  echo ""
  echo ""
  echo "✅ Keys are correct!"
  echo "🔗 Here's your link: https://forms.gle/MctUhfwe9CZMXjhy5"
  echo "🚀 Committing..."
  echo "✅ Commit successful!"
  echo ""
  echo ""
  echo ""
else
  echo "❌ Error: The keys are incorrect. Please check the content of $file."
  echo "🚫 Commit aborted."
  exit 1
fi

# Exit with a success code to allow the commit
exit 0
